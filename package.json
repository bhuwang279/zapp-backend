{
  "name": "hackathon-backend",
  "version": "1.0.0",
  "description": "Backend for hackthon",
  "main": "index.js",
  "type": "module",
  "engines": {
    "node": "12.13.1",
    "npm": "6.14.9"
  },
  "scripts": {
    "clean": "rm -rf build && mkdir build",
    "build-babel": "babel -d ./build ./src -s",
    "dev": "nodemon --exec babel-node src/index.js",
    "build": "npm run clean && npm run build-babel",
    "start": "npm run build && node ./build/index.js",
    "test": "mocha --require @babel/register 'src/**/*.spec.js'",
    "test-server": "TEST_DATABASE=tcc_test npm start",
    "db:migrate": "sequelize db:migrate",
    "db:seed": "sequelize db:seed:all",
    "db:migrate:undo": "sequelize db:migrate:undo",
    "db:migrate:undo:all": "sequelize db:migrate:undo:all",
    "db:migrate:stag": "sequelize db:migrate --env staging",
    "db:migrate:stag:undo": "sequelize db:migrate:undo --env staging",
    "db:migrate:test:undo:all": "sequelize db:migrate:undo:all --env test",
    "db:seed:stag": "sequelize db:seed:all --env staging",
    "db:migrate:prod:undo": "sequelize db:migrate:undo --env production",
    "db:migrate:prod": "sequelize db:migrate --env production",
    "db:seed:prod": "sequelize db:seed:all --env production",
    "heroku-prebuild": "export NPM_CONFIG_PRODUCTION=false; export NODE_ENV=; NPM_CONFIG_PRODUCTION=false NODE_ENV=development npm install --only=dev --dev",
    "heroku-postbuild": "export NPM_CONFIG_PRODUCTION=true; export NODE_ENV=production;"
  },
  "keywords": [],
  "author": "bhuwan@mybhutan.com",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.11.0",
    "@babel/node": "^7.10.5",
    "@babel/preset-env": "^7.11.0",
    "axios": "^0.19.2",
    "chai": "^4.2.0",
    "husky": "^4.2.5",
    "mocha": "^8.1.1",
    "nodemon": "^2.0.4",
    "sequelize-cli": "^6.2.0"
  },
  "dependencies": {
    "@babel/cli": "^7.19.3",
    "@sendgrid/mail": "^7.4.5",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "chalk": "^4.1.0",
    "cors": "^2.8.5",
    "cron": "^1.8.2",
    "dataloader": "^2.0.0",
    "dotenv": "^8.2.0",
    "email-validator": "^2.0.4",
    "express": "^4.17.1",
    "express-graphql": "^0.12.0",
    "graphql": "^15.3.0",
    "graphql-bigint": "^1.0.0",
    "graphql-fields": "^2.0.3",
    "graphql-iso-date": "^3.6.1",
    "graphql-playground-middleware-express": "^1.7.22",
    "graphql-resolvers": "^0.4.2",
    "graphql-type-json": "^0.3.2",
    "graphql-upload": "^12.0.0",
    "json2csv": "^5.0.6",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "multer": "^1.4.2",
    "nodemailer": "^6.6.3",
    "parse-bearer-token": "^1.0.1",
    "passport-local-authenticate": "^1.2.0",
    "passport-local-sequelize": "^0.9.0",
    "pg": "^8.3.0",
    "ramda": "^0.27.1",
    "response-time": "^2.3.2",
    "sequelize": "^6.6.5",
    "sequelize-values": "^1.1.0",
    "swig": "^1.4.2",
    "underscore": "^1.10.2",
    "util": "^0.12.4",
    "uuid": "^8.3.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm test",
      "pre-push": "npm test"
    }
  }
}
